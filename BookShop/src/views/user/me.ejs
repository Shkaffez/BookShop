<%- include("../partials/layout-start", {title: title}) %>
<%- include("../partials/menu") %>
<h1><%= title %></h1>

<div class="row">
    <div class="col-sm-8">
        <div class="card">
            <div class="card-body">

                <h5 class="card-title">Username: <%= user.username %></h5>
                <p class="card-text">Password: <%= user.password %></p>
                <p class="card-text">Date of registration: <%= user.registrationDate %></p>                
                <p class="card-text">ID: <%= user._id %></p> 

            </div>
        </div>
    </div>
</div>

<%- include("../partials/layout-end") %>
